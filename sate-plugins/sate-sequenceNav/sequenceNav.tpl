<div class="{{{classes}}}">
    {{#prev}}
    <div class="previous">
        <span class="prompt">{{prompt}}</span>
        <a href="{{url}}">{{name}}</a>
    </div>
    {{/prev}}
    {{#next}}
    <div class="next">
        <span class="prompt">{{prompt}}</span>
        <a href="{{url}}">{{name}}</a>
    </div>
    {{/next}}
</div>